<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
     <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
	<style name="AppTheme" parent="AppBaseTheme">
	    <!-- All customizations that are NOT specific to a particular API-level can go here. #33B5E5 -->
        <item name="android:textColor">#5d94c8</item>        
    </style> 
    

	<style name="BeevaTheme" parent="android:Theme.Holo">
	    <item name="android:textColor">@color/cl_txt_azul_beeva</item>  
	    <item name="android:windowBackground">@color/cl_negro</item> 
	</style>
	
	<color name="cl_azulclaro_and">#33b5e5</color>
	<color name="cl_azuloscuro_and">#0099cc</color>
	<color name="cl_blanco">#ffffff</color>
	<color name="cl_gris_10">#ebebeb</color>
	<color name="cl_negro">#000000</color>
	<color name="cl_turquesa_beeva">#a1d4d7</color>
	<color name="cl_verde_beeva">#d9d937</color>
	<color name="cl_azul_beeva">#A9CEE7</color>
	<color name="cl_huevo_beeva">#ffd214</color>
	<color name="cl_rosa_beeva">#ffb4b3</color>
	<color name="cl_txt_azul_beeva">#5d94c8</color>

</resources>